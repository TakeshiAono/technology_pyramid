<p id="notice"><%= notice %></p>

<h1>My Pages</h1>

  <div>
    <h2>ワーク検索</h2>
    <%= search_form_for @q, url: searches_index_path do |f| %>
      <%= f.label :title_cont, 'ワーク名' %>
      <%= f.search_field :title_cont %>
      <%= f.submit '検索' %>
    <% end %>
  </div>

  <% @users.each do |user| %>
  <td><%= user.name %></td>
  <% end %>

